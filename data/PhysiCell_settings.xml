<PhysiCell_settings version="devel-version">
	<domain>
		<x_min>-5000</x_min>
		<x_max>5000</x_max>
		<y_min>-5000</y_min>
		<y_max>5000</y_max>
		<z_min>-10</z_min>
		<z_max>10</z_max>
		<dx>20</dx>
		<dy>20</dy>
		<dz>20</dz>
		<use_2D>true</use_2D>
	</domain>
	
	<overall>
		<max_time units="min">43200</max_time> <!-- 30 days * 24 h * 60 min -->
		<time_units>min</time_units>
		<space_units>micron</space_units>
	</overall>
	
	<parallel>
		<omp_num_threads>4</omp_num_threads>
	</parallel> 
	
	<save>
		<folder>.</folder> <!-- use . for root --> 

		<full_data>
			<interval units="min">360</interval>
			<enable>true</enable>
		</full_data>
		
		<SVG>
			<interval units="min">360</interval>
			<enable>true</enable>
		</SVG>
		
		<legacy_data>
			<enable>false</enable>
		</legacy_data>
	</save>
	
	<user_parameters>
		<random_seed type="int" units="dimensionless" hidden="true">0</random_seed> 
		<!-- example parameters from the template --> 
		
		<!-- motile cell type parameters --> 
		<tumor_transition_rate type="double" units="1/min" description="the cycle rate of tumor cell">0.0022956841138659324</tumor_transition_rate>
		<tumor_max_necrosis_rate type="double" units="1/min" description="the maximum necrosis rate of tumor cell">0.002777777777777778</tumor_max_necrosis_rate>
		<elastic_rate type="double" units="1/min" description="the elastic force between parenchyma and ECM">0.05 </elastic_rate>
		<plastic_rate type="double" units="1/min" description="the plastic reorganization force of parenchyma">0.0005</plastic_rate>
		<max_ECM_displacement type="double" units="micron" description="the maximum mechanical displacement of ECM">0.75</max_ECM_displacement> 
		<tumor_max_pressue type="double" units="dimensionless" description="the maximum pressure threshold of tumor cell proliferation">1.0</tumor_max_pressue>
		
	</user_parameters>
	
	
</PhysiCell_settings>
